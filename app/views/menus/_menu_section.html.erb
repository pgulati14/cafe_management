<section class="Menu">
  <%menus.each do |menu|%>
    <li>
      <%=form_with(url: "/menus/#{menu.id}", method:"delete",class:"Menu-form") do%>
        <%=link_to menu_path(menu.id),class: "Menu-link" do%>
          <label class="Menu-container">
            <p class="Menu-text"><%= menu.menu_name %></p>
          </label>
        <%end%>
        <%=image_submit_tag 'trash-can-outline.svg'%>
        <%end%>
    </li>
  <%end%>
</section>
